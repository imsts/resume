{{!-- Виведення даних з об'єкта 'navigation' --}}
<div class="">
   {{> nav-shop }}
</div>

<main class="m-4">

   {{!-- Виведення даних з об'єкта 'header' --}}
   <section class="row">
      {{#with header}}
      <div>
         <h1 class="">{{title}}</h1>
      </div>
      {{#with button}}
      <div class="col-3 mt-3">
         <a class="btn btn-outline-secondary me-3" href="{{link}}">{{text}}</a>
      </div>
      {{/with}}
      {{/with}}
   </section>

   <div>
      <br>
      <hr>
      <br>
   </div>

   {{!-- Виведення даних з об'єкта 'goodsBlock' --}}
   <section class="">
      {{#each goodsBlock}}
      {{> cartGoodsItem}}
      {{/each}}
   </section>

   <div>
      <br>
      <hr>
      <br>
   </div>

   {{!-- Виведення даних з об'єкта 'total' --}}
   <section class="row">
      <div class="col-4">
         {{#with total}}
         <h2 class="mb-3">{{title}}</h2>
         <b class="display-5 fw-medium mb-3">{{amount}}</b>
         <br>
         <small class="pt-2 mb-4 mt-3">{{delivery}}</small>
         <br>
         {{#each buttons}}
         <button class="btn mt-3 {{#if isSuccess}} btn-success {{/if}} {{#if
               isOutline}} btn-outline-success {{/if}}" type="button" href="{{link}}">{{text}}</button>
         {{/each}}
         {{/with}}
      </div>
   </section>

   <div>
      <br>
      <hr>
      <br>
   </div>

   {{!-- Виведення даних з об'єкта 'goodsOtherBlock' --}}
   <section class="">
      <div class="">
         {{#with goodsOtherBlock}}
         <h2 class="mb-4 text-center">{{title}}</h2>
         {{> cardBlock }}
         {{/with}}
      </div>
   </section>

   <div>
      <br>
      <hr>
      <br>
   </div>

   {{!-- Виведення даних з об'єкта 'subscribe' --}}
   <section class="row">
      <div>
         {{#with subscribe}}
         {{> subscribeBlock }}
         {{/with}}
      </div>
   </section>

</main>
<div>
   <hr>
</div>

{{!-- Виведення даних з об'єкта 'footer' --}}
{{> footer-order}}