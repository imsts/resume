<div>
  {{#with main}}
  <main>
    <section>
      {{>title data='My education' big=true}}
      <ul>
        {{#each educations}}
        <li>{{name}} {{#if isEnd}} {{else}} 
          (ще в процесі..) {{/if}}</li>
        {{/each}}
      </ul>
    </section>
    <section>
      {{>title data='My certificates'}}
      <ul>
        {{#each certificates}}
        <li>{{name}} ({{id}})</li>
        {{/each}}
      </ul>
    </section>
  </main>
  {{/with}}
</div>